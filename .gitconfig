[user]
	name = David Nebauer
	email = david@nebauer.org
[credential]
	helper = cache --timeout=3600
[alias]
    ch = checkout
    co = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[init]
	templatedir = ~/.git-templates
